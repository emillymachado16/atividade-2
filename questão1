#include <stdio.h>

    int main(){
    int n;
    printf("DIGITE UM NÃšMERO: ");
    scanf("%d", &n);
    printf("\nDIVISORES DE %d: ", n);
    
    for(int i = 1; i <= n ; i++){
        if(5%i==0){
            printf("*%d, ", i);
        }
        if(n%i==0 && i!=1 && i!=5){
            printf("%d, ", i);
        }
        
    }
    printf("\nOBS: os que estao com * sao divisores de 5");
    return 0;
}
