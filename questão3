#include <stdio.h>

int main(){
    int n, i;
    printf("Digite um numero: ");
    scanf("%d", &n);
    
    if(n<=1){
        printf("\nO numero %d nao e primo", n);
    }else{
        for(i = 2; i < n; i++){
            if(n%i==0){
                printf("\nO numero %d nao e primo", n);
                break;
            }
        }
    }
        
    if(i==n){
        printf("\nO numero %d e primo", n);
    }
     
    return 0;
}
